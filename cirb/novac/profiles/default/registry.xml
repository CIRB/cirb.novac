<?xml version="1.0"?>
<registry>
    <record name="cirb.novac.novac_url">
        <field type="plone.registry.field.URI">
            <title>Waws URL</title>
            <description>Url used to Waws connection</description>
        </field>
        <value>http://ws.irisnetlab.be</value>
    </record>
    <record name="cirb.novac.json_file">
        <field type="plone.registry.field.URI">
            <title>Json URL</title>
            <description>Url used to find the public json file</description>
        </field>
        <value>http://localhost:8080/Plone/++resource++cirb.novac.scripts/test.json</value>
    </record>
    <record name="cirb.novac.rest_service">
        <field type="plone.registry.field.URI">
            <title>REST URL</title>
            <description>REST URL called to transform address in LatLon</description>
        </field>
        <value>http://services.gis.irisnetlab.be/urbis/Rest/Localize/getxycoord</value>
    </record>
</registry>