<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="cirb.novac">
      
<tal:head metal:fill-slot="head_slot">      
      <meta http-equiv="imagetoolbar" content="no" />
</tal:head>
<tal:css metal:fill-slot="style_slot">
    <!--link rel="stylesheet" tal:attributes="href string:${portal_url}/++resource++cirb.novac.scripts/novac.css" type="text/css"/-->
</tal:css>
<tal:js metal:fill-slot="javascript_head_slot"> 
    <!--script type="text/javascript" src="jquery-1.6.2.min.js"></script-->    
    <script type="text/javascript" tal:attributes="src string:${portal_url}/++resource++cirb.novac.scripts/jquery-ui.min.js"></script>
    <script type="text/javascript" tal:attributes="src string:${portal_url}/++resource++cirb.novac.scripts/novac.js"></script>

</tal:js>

<body>
       
    <div metal:fill-slot="main" tal:define="urls view/utils_url; data view/novac">
	    <div id="beschr" tal:content="context/Description">desc</div>
        <tal:block tal:condition="not: urls/error" >
            <div id="ws_urbis" tal:content="urls/urbis_url" class="hiddenStructure"> ws_urbis </div>
            <div id="ws_waws" tal:content="urls/novac_url" class="hiddenStructure"> ws_waws </div>
			<div id="json_file" tal:content="urls/json_file" class="hiddenStructure"> json_file </div>
            <div id="absolute_url" tal:content="context/absolute_url" class="hiddenStructure"> absolute_url </div>
			<div id="portal_url" tal:content="context/portal_url" class="hiddenStructure"> portal_url </div>
 			<div id="current_language" tal:content="context/Language" class="hiddenStructure"> current_language </div>
			<div id="urbis_cache_url" tal:content="urls/urbis_cache_url" class="hiddenStructure"> urbis_cache_url </div>
            <div id="map_panel"><div id="map" style="cursor: pointer;"></div></div>
           
            <div id="navigation" style="overflow: auto;">
                <div id="accordion">
                    <h3 class="slice-1"><a href="#" i18n:translate="mon_dossier">mon_dossier</a></h3>
                    <div>
                        <a tal:attributes="href data/private_url" i18n:translate="login">login</a>
                    </div>
                    <h3 class="slice-2"><a href="#" i18n:translate="search_address">search_address</a></h3>
                    <div>
                        <table>
                        <tbody>
                            <tr><td i18n:translate="street_name:">street_name:</td><td><input type="text" id="street" /></td></tr>
                            <tr><td i18n:translate="number:">number:</td><td><input type="text" id="number" /></td></tr>
                            <tr><td i18n:translate="post_code:">post_code:</td><td><input type="text" id="post_code"/></td></tr>
                        </tbody>
                        </table>
                        <input type="button" id="search_address_button" i18n:attributes="value search"/>
                    </div>
                    <h3 class="slice-3"><a href="#" i18n:translate="filtres">filtres</a></h3>
                    <div class="filter">
                        <table class="filter">
                        <tbody>
                            <tr>
                                <td i18n:attributes="class type_U"></td>
                                <td style="width:16px;"><input type="checkbox" onChange="applyDossierFilter()" checked="true" id="type_u"></td>
                                <td><label for="type_u"><tal:block i18n:translate="urbanisme">urbanisme</tal:block></td>
                            </tr>
                            <tr>
                                <td i18n:attributes="class type_L"></td>
                                <td style="width:16px;"><input type="checkbox" onChange="applyDossierFilter()" checked="true" id="type_l"></td>
                                <td><label for="type_l"><tal:block i18n:translate="lotir">lotir</tal:block></td>
                            </tr>
                        </tbody>
                        </table> 
                        <table class="filter">
                        <tbody>
                            <tr>
                                <td class="color required"></td>
                                <td style="width:16px;"><input type="checkbox" onChange="applyDossierFilter()" checked="true" id="required" /></td>
                                <td><label for="required"><tal:block i18n:translate="required">required</tal:block></td>
                            </tr>
                            <tr>
                                <td class="color granted"></td>
                                <td style="width:16px;"><input type="checkbox" onChange="applyDossierFilter()" checked="true" id="granted" /></td>
                                <td><label for="granted"><tal:block i18n:translate="granted">granted</tal:block></td>
                            </tr>
                            <tr>
                                <td class="color refused"></td>
                                <td style="width:16px;"><input type="checkbox" onChange="applyDossierFilter()" checked="true" id="refused" /></td>
                                <td><label for="refused"><tal:block i18n:translate="refused">refused</tal:block></td>
                            </tr>
                            <tr>
                                <td class="color canceled"></td>
                                <td style="width:16px;"><input type="checkbox" onChange="applyDossierFilter()" checked="true" id="canceled" /></td>
                                <td><label for="canceled"><tal:block i18n:translate="canceled">canceled</tal:block></td>
                            </tr>
                        </tbody>
                        </table>                        
                    
                        <div id="accordion2">
					        <h4><a href="#">Tous les filtres</a></h4>
					        <div>
						        <select id="commune" name="commune" style="width:180px;">
							        <option value="0" selected> Toutes les communes </option>
							        <option value="1070"> Anderlecht </option>
							        <option value="1160"> Auderghem </option>
							        <option value="1082"> Berchem-Sainte-Agathe </option>
							        <option value="1000"> Bruxelles </option>
							        <option value="1040"> Etterbeek </option>
							        <option value="1140"> Evere </option>
							        <option value="1190"> Forest </option>
							        <option value="1083"> Ganshoren </option>
							        <option value="1050"> Ixelles </option>
							        <option value="1090"> Jette </option>
							        <option value="1081"> Koekelberg </option>
							        <option value="1080"> Molenbeek-Saint-Jean </option>
							        <option value="1060"> Saint-Gilles </option>
							        <option value="1210"> Saint-Josse-ten-Noode </option>
							        <option value="1030"> Schaerbeek </option>
							        <option value="1180"> Uccle </option>
							        <option value="1170"> Watermael-Boitsfort </option>
							        <option value="1200"> Woluwe-Saint-Lambert </option>
							        <option value="1150"> Woluwe-Saint-Pierre </option>
						        </select>
						        <br /><br />
						
						        <select id="typedossier" name="typedossier" style="width:180px;">
							        <option value="0" selected> Tous les types de dossier </option>
							        <option value="1"> Abattage d'arbres </option>
                                    <option value="2"> Autorisation communale </option> 
                                    <option value="3"> Certificat d'urbanisme délivré par le FD </option>
                                    <option value="4"> Certificat d'urbanisme privé </option>
                                    <option value="5"> Enseigne </option>
                                    <option value="6"> Panneau publicitaire </option>
                                    <option value="7"> Permis de lotir </option>
                                    <option value="8"> Permis d'urbanisme délivré par le FD </option>
                                    <option value="9"> Permis d'urbanisme privé </option>
                                    <option value="10"> Permis d'urbanisme privé - procédure mixte </option>
                                    <option value="11"> Permis d'urbanisme unique délivré par le FD </option>
						        </select>
						        <br /><br />
						
						        <h5 style="font-size:1.2em;">Enquête publique ?</h5>
						        <div id="accordion3" style="font-size:1.2em;">
							        <input type="radio" name="ep" value="yes"/> Oui 
								        <div id="ep_status_div" style="display:none;">
									        <table class="filters" style="font-size:1.2em;">
										        <tr>
											        <td><input type="checkbox" name="ep_status" value="A" class="checkbox" checked="checked" /></td>
											        <td>En cours</td>
										        </tr>
										        <tr>
											        <td><input type="checkbox" name="ep_status" value="Afgesloten" class="checkbox" checked="checked" /></td>
											        <td>terminé</td>
										        </tr>
										        <tr>
											        <td><input type="checkbox" name="ep_status" value="C" class="checkbox" checked="checked" /></td>
											        <td>planifié</td>
										        </tr>
									        </table>
								        </div>
						        </div>
						        <input type="radio" name="ep" value="no"/> Non <br />
						        <input type="radio" name="ep" value="all" checked/> Tous
						
						        <h5 style="font-size:1.2em;">Date Commission de Consertation</h5>
						        de <input id="datecc_from" type="text" maxlength="10" style="width:80px;" /> dd/mm/yyyy <br />
						        &nbsp;à&nbsp;&nbsp;<input id="datecc_to" type="text" maxlength="10" style="width:80px;" /> dd/mm/yyyy 
						
						        <p><input type="checkbox"> Afficher sous forme de liste ?</p>
						        <hr class="gray" />
						        <a class="knop" href="index.html">Reset</a>&nbsp;&nbsp;<input type="button" id="search_dossiers" class="knop" value="Rechercher"/>
					        </div>
				        </div>
                    
                    </div>
                    
                </div>
            </div>
            <!--div id="results_panel" i18n:translate="results">results</div-->
        </tal:block>
        <tal:block tal:condition="urls/error" >
            <div tal:content="urls/error_text">error</div>
        </tal:block>
    </div>
</body>
</html>
